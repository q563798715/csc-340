#include<iostream>
#include<vector>
#include<fstream>
using namespace std;

int main()
{
    fstream instream;
    vector<int> temp;
    instream.open("file.txt" , ios::in | ios::out);
    if (instream.is_open())
    {
        cerr << "file is opened." << endl;
    }
    else cout << "Unable to open file";
    int filesize;
    
    instream.clear();
    instream.seekg(0, instream.end);
    filesize = (int)instream.tellg();
    cout << "The size of file1: "<< filesize <<endl;
    
    int last;
    instream.seekp (  9000 , ios:: beg );
    int c;
    while(!instream.eof())
    {
        instream >> c;
        temp.push_back ( c );
    }
    for ( int i = 0 ; i < temp.size() ; i ++)
        cout << temp[i] << endl;
    int count;
    for ( int k = 2777 ; k < 2782 ; k ++)
    {
        instream << k << endl;
        count ++;
    }
    instream.seekp (9000 + count , ios:: beg);
    int counter = 0;
    while( !instream.eof())
    {
        instream << temp[counter] << endl;
        counter ++;
    }

    instream.seekp ( 9001 , ios::beg);
    int l;
    instream >> l ;
    cout << l << endl;
    instream.close();
    return 0;
}
